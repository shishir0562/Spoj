#include <iostream>
using namespace std;
typedef long long int ll;
int main() {
	// your code goes here
	ll t;
	cin >> t;
	while(t--){
		ll n;
		cin >> n;
		ll a;
		ll sum=0;
		ll i=0;
		while(i<n){
			cin >> a;
			sum=(sum+a)%n;
			i++;
		}
		if(sum==0)
		cout << "YES\n";
		else
		cout << "NO\n";
	}
	return 0;
} 
